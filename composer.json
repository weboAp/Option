{
    "name": "weboap/option",
    "description": "Save and retreive config from db for laravel 4",
    "authors": [
        {
            "name": "weboap",
            "email": "weboap@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0"

    },
    "autoload": {
        "classmap": [
            "src/migrations",
	    "tests"
        ],
        "psr-0": {
            "Weboap\\Option": "src/"
        }
    },
    "config": {
		"preferred-install": "stable",
		"discard-changes": "true"
	},
	"minimum-stability": "stable",
	"require-dev": {
		"orchestra/testbench": "2.1.*",
		"mockery/mockery": "dev-master",
		"way/laravel-test-helpers": "dev-master",
		"doctrine/dbal": "~2.3"
		
	},
	"replace": {
	    "phpseclib/phpseclib": "0.3.*"
	}
	
}